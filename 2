#include <stdio.h>

// Функція для обчислення НОД
int gcd(int a, int b) {
    if (b == 0)
        return a;
    return gcd(b, a % b);
}

// Функція для обчислення НСК
int lcm(int a, int b) {
    return (a * b) / gcd(a, b);
}

int main() {
int n;
scanf("%d", &n);

    int numbers[n];

    // Зчитування ряду чисел
for (int i = 0; i < n; i++) {
scanf("%d", &numbers[i]);
    }

    // Обчислення НСК за допомогою поступового обчислення НСК пар чисел
    int result = numbers[0];
    for (int i = 1; i < n; i++) {
        result = lcm(result, numbers[i]);
    }

    printf("%d\n", result);

    return 0;
}
